<h3>商品一覧</h3>
<%= link_to new_admin_item_path do %>
	<h1><i class="fas fa-plus-circle"></i></h1>
<% end %>
<table class="table">
	<th>商品ID</th>
	<th>商品名</th>
	<th>税抜価格</th>
	<th>ジャンル</th>
	<th>ステータス</th>
<br>
<% @items.each do |i| %>
	<tr>
		<td><%= i.id %></td>
		<td><%= link_to i.name, admin_item_path(i) %></td>
		<td><%= i.price %></td>
		<td><%= i.genre_id %></td>
		<td><%= i.is_selling %></td>
	</tr>
<% end %>
</table>